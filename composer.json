{
    "name": "peibin/laravel-snowflake",
    "description": "Peibin snowflake component for Laravel.",
    "license": "MIT",
    "keywords": [
        "php",
        "snowflake"
    ],
    "authors": [
        {
            "name": "Peibin Zhu",
            "email": "zhupeibin1207@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "peibin/laravel-redis-pool": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PeibinLaravel\\Snowflake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeibinLaravelTest\\Snowflake\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PeibinLaravel\\Snowflake\\SnowflakeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
